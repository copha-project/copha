{
    "name": "copha",
    "version": "0.0.15",
    "description": "a general framework for running custom network tasks.",
    "keywords": [
        "copha",
        "cli",
        "tools",
        "log",
        "logs",
        "koa",
        "cron",
        "nodejs",
        "framework",
        "selenium",
        "crawler",
        "puppeteer"
    ],
    "repository": "https://github.com/copha-project/copha.git",
    "homepage": "https://copha.net",
    "bugs": {
        "url": "https://github.com/copha-project/copha/issues",
        "email": "contacts@copha.net"
    },
    "bin": {
        "copha": "./dist/bin/index.js"
    },
    "scripts": {
        "dev": "cross-env COPHA_DEBUG=1 ts-node --files -- ./src/bin/dev.ts",
        "start": "ts-node --files -- ./src/bin/index.ts",
        "test": "node ./test/index.js",
        "build": "gulp",
        "clear": "node ./dev/clear.js",
        "np": "npm version from-git --force --no-git-tag-version --no-commit-hooks --allow-same-version",
        "init-submodule": "git submodule update --init",
        "update-submodule": "git submodule update --remote --merge"
    },
    "main": "./dist/index.js",
    "author": "aong",
    "license": "MIT",
    "os": [
        "darwin",
        "linux",
        "win32"
    ],
    "devDependencies": {
        "@types/archiver": "^5.3.1",
        "@types/command-exists": "^1.2.0",
        "@types/koa-router": "^7.4.4",
        "@types/node": "^16.11.12",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "cross-env": "^7.0.3",
        "del": "^6.0.0",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.2.0",
        "gulp": "^4.0.2",
        "gulp-typescript": "^6.0.0-alpha.1",
        "merge2": "^1.4.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "archiver": "^5.3.0",
        "axios": "^0.26.1",
        "cli-progress": "^3.4.0",
        "command-exists": "^1.2.9",
        "commander": "^8.1.0",
        "compare-versions": "^4.1.3",
        "koa": "^2.13.1",
        "koa-compose": "^4.1.0",
        "koa-router": "^10.0.0",
        "ncp": "^2.0.0",
        "open-in-editor": "^2.2.0",
        "pidusage": "^2.0.20",
        "uni-utils": "^1.0.19",
        "unzipper": "^0.10.11",
        "winston": "^3.2.1"
    }
}
